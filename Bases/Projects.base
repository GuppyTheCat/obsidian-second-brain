filters:
  and:
    - '!file.path.contains("Templates/")'
    - categories.contains(link("Projects"))
    - file.name != link("–ü—Ä–∏–º–µ—Ä –ø—Ä–æ–µ–∫—Ç–∞ - –û—Ä–≥–∞–Ω–∏–∑–∞—Ü–∏—è –æ—Ç–ø—É—Å–∫–∞")
properties:
  file.name:
    displayName: project
  status:
    displayName: status
  priority:
    displayName: ‚ö°
  area:
    displayName: area
  folder:
    displayName: üìÅ
  list:
    displayName: üìã
  energy_level:
    displayName: üîã
  start_date:
    displayName: start
