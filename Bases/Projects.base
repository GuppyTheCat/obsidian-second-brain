  year:
    displayName: year
  url:
    displayName: url
  cover:
    displayName: cover
  icon:
    displayName: icon
  color:
    displayName: color
views:
  - type: table
    name: All
    groupBy:
      property: area
      direction: ASC
    order:
      - file.name
      - status
    sort:
      - property: status
        direction: ASC
      - property: year
        direction: DESC
    columnSize:
      file.name: 576
  - type: table
    name: Personal
    filters:
      and:
        - area == "Personal"
    order:
      - file.name
      - status
      - year
      - url
    sort:
      - property: status
        direction: ASC
      - property: year
        direction: DESC
    columnSize:
      file.name: 209
      status: 199
